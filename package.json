{
  "name": "attendance-system",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vue-cli-service serve --mode development",
    "build:test": "vue-cli-service build --mode development",
    "build:prod": "vue-cli-service build --mode production",
    "gz:test": "npm run build:test && bash ./bash/gz.sh",
    "gz:prod": "npm run build:prod && bash ./bash/gz.sh",
    "release:test": "npm run gz:test && bash ./bash/test.sh",
    "release:prod": "npm run gz:prod && bash ./bash/prod.sh",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "animate.css": "^3.7.2",
    "axios": "^0.19.0",
    "core-js": "^2.6.5",
    "element-ui": "^2.10.0",
    "jquery": "^3.4.1",
    "js-md5": "^0.7.3",
    "moment": "^2.24.0",
    "qs": "^6.7.0",
    "register-service-worker": "^1.6.2",
    "v-dialogs": "^2.0.2",
    "vue": "^2.6.10",
    "vue-cookies": "^1.5.13",
    "vue-router": "^3.0.3",
    "vuedraggable": "^2.23.0",
    "vuex": "^3.0.1",
    "webStorage": "^2.1.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.8.0",
    "@vue/cli-plugin-eslint": "^3.8.0",
    "@vue/cli-plugin-pwa": "^3.8.0",
    "@vue/cli-service": "^3.8.0",
    "@vue/eslint-config-standard": "^4.0.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "less": "^3.0.4",
    "less-loader": "^4.1.0",
    "node-sass": "^4.12.0",
    "sass-loader": "^7.1.0",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/standard"
    ],
    "rules": {
      "arrow-parens": 0,
      "generator-star-spacing": 0,
      "no-debugger": 0,
      "indent": 0,
      "comma-dangle": 0,
      "object-curly-spacing": 0
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
