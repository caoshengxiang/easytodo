{
  "name": "easyto",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vue-cli-service serve --mode development",
    "build:test": "vue-cli-service build --mode test",
    "build:prod": "vue-cli-service build --mode production",
    "gz:test": "npm run build:test && bash ./bash/gz.sh",
    "gz:prod": "npm run build:prod && bash ./bash/gz.sh",
    "release:test": "npm run gz:test && bash ./bash/test.sh",
    "release:prod": "npm run gz:prod && bash ./bash/prod.sh",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "@jiaminghi/data-view": "^2.7.3",
    "animate.css": "^3.7.2",
    "axios": "^0.19.2",
    "core-js": "^3.4.4",
    "echarts": "^4.6.0",
    "element-ui": "^2.13.0",
    "jquery": "^3.4.1",
    "js-md5": "^0.7.3",
    "moment": "^2.24.0",
    "qs": "^6.9.1",
    "register-service-worker": "^1.6.2",
    "swiper": "^5.3.6",
    "v-dialogs": "2.0.2",
    "vue": "^2.6.10",
    "vue-awesome-swiper": "^2.5.4",
    "vue-baidu-map": "^0.21.22",
    "vue-cookies": "^1.6.1",
    "vue-count-to": "^1.0.13",
    "vue-router": "^3.1.5",
    "vuedraggable": "^2.23.2",
    "vuex": "^3.1.2",
    "webStorage": "^2.1.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.1.0",
    "@vue/cli-plugin-eslint": "^4.1.0",
    "@vue/cli-service": "^5.0.8",
    "@vue/eslint-config-standard": "^5.1.0",
    "babel-eslint": "^10.0.3",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "less": "^3.10.3",
    "less-loader": "^5.0.0",
    "node-sass": "^4.13.1",
    "sass-loader": "^8.0.2",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "arrow-parens": 0,
      "generator-star-spacing": 0,
      "no-debugger": 0,
      "indent": 0,
      "comma-dangle": 0,
      "object-curly-spacing": 0,
      "no-unused-vars": 0
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
